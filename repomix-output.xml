      )}

      <div
        className="overflow-y-auto bg-muted p-10 min-h-[90vh] h-full"
        style={
          {
            maxHeight: "calc(100vh - 64px)",  
          }
        }
      >
        {children}
      </div>
    </div>
  );
};

export default DashboardLayout;
